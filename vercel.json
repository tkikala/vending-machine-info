{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@5.3.7"
    }
  },
  "rewrites": [
    {
      "source": "/api",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 